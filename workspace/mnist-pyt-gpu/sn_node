#!/usr/bin/env bash
./scripts/bin/run-sn -it --rm --name=sn1 --network=host-1-net --host-ip=192.168.33.102 --sentinel --sn-p2p-port=30303 --sn-api-port=30304 --key=workspace/mnist-pyt-gpu/cert/sn-1-key.pem --cert=workspace/mnist-pyt-gpu/cert/sn-1-cert.pem --capath=workspace/mnist-pyt-gpu/cert/ca/capath --apls-ip=192.168.33.102 --apls-port 5000

./scripts/bin/run-swop -it --rm --name=swop1 --network=host-1-net --usr-dir=workspace/mnist-pyt-gpu/swop --profile-file-name=swop1_profile.yaml --key=workspace/mnist-pyt-gpu/cert/swop-1-key.pem --cert=workspace/mnist-pyt-gpu/cert/swop-1-cert.pem --capath=workspace/mnist-pyt-gpu/cert/ca/capath -e http_proxy= -e https_proxy= --apls-ip=192.168.33.102 --apls-port 5000 -e SWOP_KEEP_CONTAINERS=True

./scripts/bin/run-swci -it --rm --name=swci1 --network=host-1-net --usr-dir=workspace/mnist-pyt-gpu/swci --init-script-name=swci-init --key=workspace/mnist-pyt-gpu/cert/swci-1-key.pem --cert=workspace/mnist-pyt-gpu/cert/swci-1-cert.pem --capath=workspace/mnist-pyt-gpu/cert/ca/capath -e http_proxy= -e https_proxy= --apls-ip=192.168.33.102 --apls-port 5000./scripts/bin/run-swci -it --rm --name=swci1 --network=host-1-net --usr-dir=workspace/mnist-pyt-gpu/swci --init-script-name=swci-init --key=workspace/mnist-pyt-gpu/cert/swci-1-key.pem --cert=workspace/mnist-pyt-gpu/cert/swci-1-cert.pem --capath=workspace/mnist-pyt-gpu/cert/ca/capath -e http_proxy= -e https_proxy= --apls-ip=192.168.33.102 --apls-port 5000
